// use current mini point and temp profit
public class Solution {
    public int maxProfit(int[] prices) {
        if(prices.length<1)
            return 0;
        int min = prices[0];
        int maxprofit = 0;
        for (int i = 0 ; i<prices.length; i++){
            if (prices[i] < min)
                min = prices[i];
            int between = prices[i] - min;
            if (between > maxprofit)
                maxprofit = between;
        }
        return maxprofit;
    }
}
