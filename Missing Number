public class Solution {
    public int missingNumber(int[] nums) {
       HashMap<Integer,Integer> hm = new HashMap<Integer,Integer>();
       for(int i = 0; i<= nums.length; i++){
           hm.put(i,0);
       }
       for(int i : nums){
            hm.put(i,1);   
       }
       int k = 0;
       for(int i: hm.keySet()){
           if(hm.get(i) ==0)
                k = i;
       }
       return k;
    }
}
